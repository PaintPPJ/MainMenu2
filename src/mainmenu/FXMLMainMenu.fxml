<fx:root style="-fx-background-color: transparent;" stylesheets="@fxmlmainmenu.css" type="VBox" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="mainmenu.FXMLMainMenuController">
